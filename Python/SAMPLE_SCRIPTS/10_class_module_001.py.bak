def add(*args):
	sum_ = 0
	for i in  args:
		 sum_ = sum_ +  i
	return i

def sub(*args):
	sub_=args[0]
	for i in range(1, len(args)):
		sub_ -=args[i]

	return sub_

def mul(*args):
	mul_=args[0]
	for i in range(1, len(args)):
		mul_ -=args[i]
	return mul_


